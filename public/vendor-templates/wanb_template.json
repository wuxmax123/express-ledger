{
  "vendor": "Wanb",
  "file_patterns": [".*万邦.*", ".*Wanb.*", ".*wanb.*"],
  "sheets": [
    {
      "match": {
        "name_regex": "^(?!.*(目录|总目录|国家维度|偏远|邮编|附加费|清单|服务|分区|说明|税率|HPRA|药事法|紧急|保价|签名)).+$"
      },
      "type": "rate_card",
      "whitelist_keywords": ["万邦", "挂号", "平邮", "大货", "专线", "服装", "化妆", "全球"],
      "header_row_max_search": 8,
      "meta_extract": {
        "channel_code": {
          "source": "cell_search",
          "row_range": [0, 12],
          "col_range": [0, 10],
          "regex": "(?:产品代码|渠道代码|Channel Code|Product Code)[:：\\s]*([A-Za-z0-9\\-_/]+)"
        },
        "effective_date": {
          "source": "cell_search",
          "row_range": [0, 12],
          "col_range": [0, 10],
          "regex": "(?:生效日期|有效期|Effective Date)[:：\\s]*(\\d{4}[/-]\\d{1,2}[/-]\\d{1,2})"
        }
      },
      "header_aliases": {
        "country": ["国家/地区", "国家", "目的地", "Country", "Destination"],
        "zone": ["分区", "区域", "分区代码", "Zone"],
        "eta": ["参考时效", "时效", "派送时效", "ETA", "工作日"],
        "weight_range": ["重量(KG)", "重量区间", "重量段", "Weight", "Weight Range", "重量"],
        "weight_from": ["起重", "From(kg)", "下限(kg)", "计费始重"],
        "weight_to": ["至重", "To(kg)", "上限(kg)", "计费至重"],
        "rounding_step": ["进位制(KG)", "进位制", "计费进位"],
        "min_chargeable_weight": ["最低计费重(KG)", "最低计费重", "最低计费重量"],
        "price_per_kg": ["运费(RMB/KG)", "运费(元/KG)", "价格", "单价", "Price", "运费"],
        "register_fee": ["挂号费(RMB/票)", "挂号费", "处理费", "Register Fee", "挂号费(RMB)"],
        "currency": ["币种", "Currency"]
      },
      "weight_patterns": [
        "(?P<l>\\d+(\\.\\d+)?)\\s*[<]\\s*W\\s*[<=]\\s*(?P<u>\\d+(\\.\\d+)?)",
        "^0\\s*[<]\\s*W\\s*[<=]\\s*(?P<u>\\d+(\\.\\d+)?)$",
        "^(?P<l>\\d+(\\.\\d+)?)\\s*[<]\\s*W\\s*[<=]\\s*MAX$"
      ],
      "eta_patterns": [
        "(\\d+)\\s*-\\s*\\d+",
        "(\\d+)天",
        "(\\d+)\\s*day(s)?"
      ],
      "text_normalization": {
        "＜": "<",
        "≤": "<=",
        "：": ":",
        "　": " ",
        "–": "-",
        "（": "(",
        "）": ")"
      },
      "confidence_threshold": 70
    }
  ]
}